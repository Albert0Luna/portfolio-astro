---
import About from "../../components/About.astro";
import Experience from "../../components/Experience.astro";
import Hero from "../../components/Hero.astro";
import Projects from "../../components/Projects.astro";
import { SITE_CONTENT_ES } from "../../config";
import Layout from "../../layouts/Layout.astro";
---

<Layout>
  <Hero {...SITE_CONTENT_ES.hero} />
  <Projects projects={SITE_CONTENT_ES.projects} />
  <Experience experience={SITE_CONTENT_ES.experience} />
  <About {...SITE_CONTENT_ES.about} name={SITE_CONTENT_ES.hero.name} />
</Layout>
